<h1>Hardcover books published in 2018</h1>
<table>
<thead>
  <tr>
    <th>Title</th>
    <th>Release Date</th>
  </tr>
</thead>
<tbody>  
  <% @books.each do |book| %>
    <tr>
      <td><%= link_to book.title, book %></td>
      <td><%= book.release_date %></td>
    </tr>
  <% end %>
</tbody>  
</table>

<%= link_to 'Return to book list', books_path %>
